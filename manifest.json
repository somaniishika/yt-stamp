{
  "name": "Youtube Bookmarks",
  "version": "1.0",
  "description": "Bookmark youtube videos at current time stamp",
  "permissions":[
      "activeTab",
      "storage"
  ],
  "content_scripts": [{
      "matches": ["*://*.youtube.com/*"],
      "js": ["app.js"]
  }],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "images/icon16.png",
          "48": "images/icon48.png",
          "128": "images/icon128.png"
      }
  },
  "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
  },
  "manifest_version": 3
}